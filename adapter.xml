<adapter>
  <unique-id>naughty-roentgen</unique-id>
  <start-up-event-imp>com.adaptris.core.event.StandardAdapterStartUpEvent</start-up-event-imp>
  <heartbeat-event-imp>com.adaptris.core.HeartbeatEvent</heartbeat-event-imp>
  <shared-components>
    <connections/>
    <services/>
  </shared-components>
  <event-handler class="default-event-handler">
    <unique-id>DefaultEventHandler</unique-id>
    <connection class="null-connection">
      <unique-id>tiny-easley</unique-id>
    </connection>
    <producer class="null-message-producer">
      <unique-id>condescending-stallman</unique-id>
    </producer>
  </event-handler>
  <message-error-handler class="null-processing-exception-handler">
    <unique-id>condescending-hodgkin</unique-id>
  </message-error-handler>
  <failed-message-retrier class="no-retries">
    <unique-id>prickly-colden</unique-id>
  </failed-message-retrier>
  <channel-list>
    <channel>
      <consume-connection class="null-connection">
        <unique-id>reverent-allen</unique-id>
      </consume-connection>
      <produce-connection class="null-connection">
        <unique-id>agitated-franklin</unique-id>
      </produce-connection>
      <workflow-list>
        <standard-workflow>
          <consumer class="null-message-consumer">
            <unique-id>angry-volhard</unique-id>
          </consumer>
          <service-collection class="service-list">
            <unique-id>elegant-torvalds</unique-id>
            <services>
              <aggregating-fs-consume-service>
                <unique-id>furious-bassi</unique-id>
                <fs-consumer>
                  <message-aggregator class="xml-document-aggregator">
                    <merge-implementation class="xml-insert-node">
                      <xpath-to-parent-node>//root</xpath-to-parent-node>
                    </merge-implementation>
                  </message-aggregator>
                  <destination class="consume-destination-from-metadata">
                    <destination-metadata-key>temp</destination-metadata-key>
                  </destination>
                </fs-consumer>
              </aggregating-fs-consume-service>
            </services>
          </service-collection>
          <producer class="null-message-producer">
            <unique-id>small-goldberg</unique-id>
          </producer>
          <produce-exception-handler class="null-produce-exception-handler"/>
          <unique-id>high-bhabha</unique-id>
        </standard-workflow>
      </workflow-list>
      <unique-id>sleepy-gates</unique-id>
    </channel>
  </channel-list>
  <message-error-digester class="standard-message-error-digester">
    <unique-id>ErrorDigest</unique-id>
    <digest-max-size>100</digest-max-size>
  </message-error-digester>
</adapter>